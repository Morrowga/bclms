{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "alexusmai/laravel-file-manager": "^3.0",
        "escolasoft/h5p-core": "dev-master",
        "escolasoft/h5p-editor": "dev-master",
        "guzzlehttp/guzzle": "^7.2",
        "hansschouten/laravel-pagebuilder": "0.22",
        "hareom284/laravel-h5p": "1.0.x-dev",
        "inertiajs/inertia-laravel": "^0.6.9",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "laravelcollective/html": "^6.4",
        "maatwebsite/excel": "^3.1",
        "nunomaduro/collision": "^6.0",
        "spatie/ignition": "^1.4",
        "spatie/laravel-medialibrary": "^10.7",
        "stancl/tenancy": "^3.7",
        "tightenco/ziggy": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^7.7",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.23",
        "phpunit/phpunit": "^9.0",
        "spatie/laravel-ignition": "^1.6"
    },
    "autoload": {
        "files": [
            "src/Common/Infrastructure/helpers.php"
        ],
        "psr-4": {
            "Src\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "vendor/escolasoft/h5p-core/h5p-default-storage.class.php",
            "vendor/escolasoft/h5p-core/h5p-development.class.php",
            "vendor/escolasoft/h5p-core/h5p-event-base.class.php",
            "vendor/escolasoft/h5p-core/h5p-file-storage.interface.php",
            "vendor/escolasoft/h5p-core/h5p.classes.php",
            "vendor/escolasoft/h5p-editor/h5peditor-ajax.class.php",
            "vendor/escolasoft/h5p-editor/h5peditor-ajax.interface.php",
            "vendor/escolasoft/h5p-editor/h5peditor-file.class.php",
            "vendor/escolasoft/h5p-editor/h5peditor-storage.interface.php",
            "vendor/escolasoft/h5p-editor/h5peditor.class.php"
        ]

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
